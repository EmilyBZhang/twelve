{
  "expo": {
    "name": "twelve",
    "slug": "twelve",
    "privacy": "public",
    "sdkVersion": "35.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/12.png",
    "splash": {
      "image": "./src/assets/images/twelve-padded-title.png",
      "resizeMode": "contain",
      "backgroundColor": "#00ffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.bradonzhang.twelve",
      "icon": "./src/assets/images/12.png"
    },
    "android": {
      "package": "com.bradonzhang.twelve",
      "icon": "./src/assets/images/12-circle.png",
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/12-circle.png",
        "backgroundColor": "#00ffff"
      }
    },
    "description": "A puzzle game about collecting twelve dots... with a special twist in each level!",
    "githubUrl": "https://github.com/BradonZhang/twelve"
  }
}